      "max-length": 5
    },
    "ISBN" : {
      "required": true,
      "regex": "[0-9]+"
    },
    "Published_year": {
      "required": true,
      "regex": "[0-9]+"
    },
    "Nos_Available": {
      "required": true,
      "regex": "[0-9]+"
    }
  },
  "/add-category": {
    "Category_Name": {
      "required": true,
      "regex": "[a-zA-Z ]*$"
    }
  }

}