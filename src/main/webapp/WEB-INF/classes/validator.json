    "Book_Title": {
      "required": true,
      "regex": "^[a-zA-Z0-9 ]*$"

    },
    "Author_Name": {
      "required": true,
      "regex":"^[a-zA-Z ]*$"
    },
    "Category_Id": {
      "required": true,
      "regex": "[0-9]+",
